<?xml version="1.0" encoding="utf-8" ?>
<xcomos:PageTemplate xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:xcomos="clr-namespace:XComosMobile;assembly=XComosMobile"
             ControlTemplate="{StaticResource PageTemplate}"
             xmlns:q="clr-namespace:XComosMobile.Pages.controls;assembly=XComosMobile"
             xmlns:local="clr-namespace:XComosMobile.Pages.maintenance;assembly=XComosMobile"
             x:Class="XComosMobile.Pages.maintenance.WorkPackageDetail"
             Title="Packotes">
    <StackLayout HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand"
                   Orientation="Vertical" Padding="0,0,0,0">
        <ScrollView x:Name="m_Panel" HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand"
          Orientation="Both" Padding="0,5,0,0">
        </ScrollView>
        <Button HorizontalOptions="End" Text="Salvar" Clicked="OnSaveClicked"/>
    </StackLayout>
</xcomos:PageTemplate>